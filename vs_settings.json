  "markdown-preview-enhanced.imageUploader": "imgur",
  "files.autoSave": "afterDelay",
  "editor.linkedEditing": true,
  "html-css-class-completion.enableEmmetSupport": true,
  "editor.quickSuggestions": {
    "other": "on",
